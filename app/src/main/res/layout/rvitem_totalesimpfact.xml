<?xml version="1.0" encoding="utf-8"?>
 <androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#76aaea"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/lblTituloTotalImpFacItem"
        android:layout_width="0dp"
        android:textStyle="bold"
        android:textSize="16sp"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtTotalImpFacItem"
        android:text="Total">
    </TextView>

    <TextView
        android:id="@+id/txtTotalImpFacItem"
        android:textStyle="bold"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:layout_constraintStart_toEndOf="@+id/lblTituloTotalImpFacItem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="00999.50">
    </TextView>



</androidx.constraintlayout.widget.ConstraintLayout>
